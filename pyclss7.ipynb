    "#Exception handling\n",
    "try:\n",
    "    num=int(input(\"Enter a number:\"))\n",
    "    result=10/num\n",
    "except ZeroDivisionError:\n",
    "    print(\"Error:Division by zero is not allowed!\")\n",
    "else:\n",
    "    print(f\"Result:{result}\")\n",
    "finally:\n",
    "    print(\"program execution completed\")"
   ]
